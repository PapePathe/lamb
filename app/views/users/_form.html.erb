<%= semantic_form_for @user do |f| %>

	<%= f.inputs do %>
		<%= f.input :nom_complet %>
		<%= f.input :email %>
		<%= f.input :password %>
		<%= f.input :password_confirmation %>
	<% end %>

	<%= f.actions do %>
		<%= f.action :submit, label: 'terminer', :button_html => { :id => "terminer", :value => 'TERMINER', :class => 'btn primary' } %>
	<% end %>

<% end %>
